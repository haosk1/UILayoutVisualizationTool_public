<? xml version = "1.0" encoding = "UTF-8" ?>
<Resource>
    <RelativeLayout
        Id="@+Id/LayoutParent"
        LayoutWidth="FillParent"
        LayoutHeight="WrapContent">
        <!-- 充电站转途径地 -->
        <RelativeLayout
            Id="@+Id/RelativeLayoutRecommended_ChargingStation"
            LayoutWidth="506"
            LayoutHeight="180"
            LayoutLeftMargin="32">
            <!--1级List 推荐充电站 长按识别使用-->
            <LinearLayout
                Id="@+Id/Lnr_Recommended_ChargingStation"
                LayoutWidth="392"
                LayoutHeight="FillParent"
                Clickable="true"
                Focusable="true"
                HorizontalGravity="Left"
                VerticalGravity="Center"
                Orientation="Vertical"
                Background="@Drawable/card_right_angle_conrners_tripview_bg">
                <RelativeLayout
                    LayoutWidth="FillParent"
                    LayoutHeight="178"
                    DuplicateParentState="true">
                    <!--推荐充电站图标-->
                    <ImageActor
                        Id="@+Id/RecommendedImageType_ChargingStation"
                        LayoutWidth="64"
                        LayoutHeight="64"
                        LayoutLeftMargin="20"
                        LayoutTopMargin="58" />
                    <LinearLayout
                        LayoutHeight="FillParent"
                        HorizontalGravity="Left"
                        DuplicateParentState="true"
                        Orientation="Vertical"
                        LayoutWidth="242"
                        LayoutTopMargin = "-4"
                        LayoutLeftMargin="100">
                        <LinearLayout
                            LayoutHeight="40"
                            LayoutWidth="FillParent"
                            DuplicateParentState="true"
                            LayoutTopMargin="24"
                            VerticalGravity="Center">
                            <!--推荐充电站名称-->
                            <TextActor
                                Id="@+Id/TxtRecommendedTitle_ChargingStation"
                                Style="@Style/cns_TripView_Title"
                                LayoutWidth="240"
                                LayoutHeight="40"
                                DuplicateParentState="true" />
                        </LinearLayout>
                        <LinearLayout
                            LayoutHeight="40"
                            LayoutWidth="FillParent"
                            LayoutTopMargin="10"
                            DuplicateParentState="true"
                            VerticalGravity="Center">
                            <LinearLayout
                                LayoutHeight="40"
                                LayoutWidth="WrapContent"
                                DuplicateParentState="true">
                                <!--推荐充电站详细地址信息-->
                                <TextActor
                                    Id="@+Id/TxtRecommendedDetailAddress_ChargingStation"
                                    Style="@Style/cns_TripView_Title"
                                    LayoutWidth="WrapContent"
                                    LayoutHeight="40"
                                    DuplicateParentState="true" />
                                <TextActor
                                    Id="@+Id/TxtRecommendedDetailAddressUnit"
                                    Style="@Style/cns_TripView_Address"
                                    LayoutWidth="WrapContent"
                                    LayoutHeight="40"
                                    DuplicateParentState="true" />
                            </LinearLayout>
                            <TextActor
                                Id="@+Id/TxtRecommendedDetailAddress_ChargingStationtime"
                                Style="@Style/cns_TripView_Title"
                                LayoutWidth="100"
                                LayoutLeftMargin="20"
                                LayoutHeight="40"
                                DuplicateParentState="true" />
                        </LinearLayout>
                        <LinearLayout
                            LayoutHeight="48"
                            LayoutTopMargin="8"
                            LayoutLeftMargin="3"
                            LayoutWidth="FillParent"
                            DuplicateParentState="true"
                            VerticalGravity="Center">
                            <!--电量图标-->
                            <ImageActor
                                Id="@+Id/Img_ChargingPower1"
                                LayoutWidth="48"
                                LayoutHeight="48"
                                LayoutTopMargin="-2"
                                LayoutLeftMargin="1"
                                DuplicateParentState="true"
                                Background="@Drawable/newpower_tripview" />
                            <!--充电时间-->
                            <LinearLayout
                                Id="@+Id/Txt_ChargingTime2"
                                LayoutHeight="40"
                                LayoutTopMargin="3"
                                LayoutLeftMargin="3"
                                LayoutWidth="FillParent"
                                DuplicateParentState="true">
                                <TextActor
                                    Id="@+Id/Txt_ChargingTime2_HourNum"
                                    LayoutWidth="WrapContent"
                                    LayoutHeight="40"
                                    DuplicateParentState="true"
                                    Style="@Style/cns_TripView_Title" />
                                <TextActor
                                    Id="@+Id/Txt_ChargingTime2_HourUnit"
                                    LayoutWidth="WrapContent"
                                    LayoutHeight="40"
                                    Style="@Style/cns_TripView_Address"
                                    DuplicateParentState="true"
                                    Text="@String/MX_LANG_Detail_Description_Display_Rules_of_Time_TEXT_Hour" />
                                <TextActor
                                    Id="@+Id/Txt_ChargingTime2_MinNum"
                                    LayoutWidth="WrapContent"
                                    LayoutHeight="40"
                                    DuplicateParentState="true"
                                    Style="@Style/cns_TripView_Title" />
                                <TextActor
                                    Id="@+Id/Txt_ChargingTime2_MinUnit"
                                    LayoutWidth="WrapContent"
                                    LayoutHeight="40"
                                    Style="@Style/cns_TripView_Address"
                                    DuplicateParentState="true"
                                    Text="@String/MX_LANG_Detail_Description_Display_Rules_of_Time_TEXT_Min1" />
                            </LinearLayout>
                        </LinearLayout>
                    </LinearLayout>
                </RelativeLayout>
            </LinearLayout>
            <RelativeLayout
                LayoutWidth = "132"
                LayoutHeight = "132"
                LayoutAlignParentRight = "true"
                LayoutRightMargin = "-10"
                LayoutTopMargin = "24">
                <!-- 分割线 -->
                <ImageActor
                    LayoutLeftMargin="8"
                    LayoutWidth="2"
                    LayoutHeight="56"
                    LayoutCenterVertical="true"
                    Background="@Drawable/card_list_vertical_line" />
                <!--List折叠/展开 icon-->
                <ImageButton
                    Id="@+Id/ImgListFold_Charging2"
                    LayoutHeight="96"
                    LayoutWidth="96"
                    LayoutCenterInParent="true"
                    DuplicateParentState="true"
                    Drawable="@Drawable/arrow_up_cardlist_tripView"
                    Background="@Drawable/h64_background_normal_clear_list" />
            </RelativeLayout>
            <!--List分割线-->
            <ImageActor
                LayoutWidth="FillParent"
                LayoutHeight="2"
                LayoutAlignParentBottom = "true"
                Background="@Drawable/radio_button_line" />
        </RelativeLayout>
        <RelativeLayout
            Id = "@+Id/RelativeLayoutVia_GP"
            LayoutWidth = "506"
            LayoutHeight = "128"
            LayoutLeftMargin = "32">
            <!--1级List 途经点 长按识别使用-->
            <LinearLayout
                Id = "@+Id/LayoutVia_GP"
                LayoutWidth = "392"
                LayoutHeight = "FillParent"
                Clickable = "true"  
                Focusable="true"
                HorizontalGravity = "Left"
                VerticalGravity = "Center"
                Orientation = "Vertical"
                Background = "@Drawable/card_right_angle_conrners_tripview_bg">
                <RelativeLayout
                    LayoutWidth = "FillParent"
                    LayoutHeight = "126"
                    DuplicateParentState = "true">
                    <!--经由地图标-->
                    <ImageActor
                        Id = "@+Id/PointImageType1_GP"
                        LayoutWidth = "64"
                        LayoutHeight = "64"
                        LayoutLeftMargin = "20"
                        LayoutTopMargin = "38"
                        DuplicateParentState = "true"/>
                    <LinearLayout
                        LayoutWidth = "275"
                        LayoutHeight = "95"
                        HorizontalGravity = "Left"
                        DuplicateParentState = "true"
                        LayoutTopMargin = "28"
                        LayoutLeftMargin = "98"
                        Orientation = "Vertical">
                        <LinearLayout
                            LayoutHeight = "40"
                            DuplicateParentState = "true"
                            LayoutWidth = "FillParent">
                        <!--经由地名称-->
                        <TextActor
                            Id = "@+Id/TxtPointTitle_GP"
                            Style = "@Style/cns_TripView_Title"
                            LayoutWidth = "282"
                            LayoutHeight = "40"
                            DuplicateParentState = "true"/>
                        </LinearLayout>
                        <LinearLayout
                            LayoutHeight = "52"
                            DuplicateParentState = "true"
                            LayoutTopMargin = "-5" 
                            LayoutWidth = "FillParent">
                            <!--经由地距离信息-->
                            <TextActor
                                Id = "@+Id/TxtDistance_GP"
                                Style = "@Style/cns_TripView_Title"
                                LayoutWidth = "WrapContent"
                                LayoutHeight = "FillParent"
                                DuplicateParentState = "true"/>
                            <TextActor
                                Id = "@+Id/TxtDistanceUnit_GP"
                                Style = "@Style/cns_TripView_Address"
                                LayoutWidth = "WrapContent"
                                LayoutHeight = "FillParent"
                                DuplicateParentState = "true"/>
                            <!--经由地时间信息-->
                            <TextActor
                                Id = "@+Id/TxtTime_GP"
                                Style = "@Style/cns_TripView_Title"
                                LayoutWidth = "WrapContent"
                                LayoutHeight = "FillParent"
                                LayoutLeftMargin = "30"
                                DuplicateParentState = "true"/>
                            <!--经由地 - 地址信息-->
                            <TextActor
                                Id = "@+Id/TxtDetail_GP"
                                Style = "@Style/cns_TripView_Address"
                                LayoutWidth = "WrapContent"
                                LayoutHeight = "FillParent"
                                LayoutLeftMargin = "9"
                                DuplicateParentState = "true"/>
                        </LinearLayout>
                    </LinearLayout>
                </RelativeLayout>
            </LinearLayout>
            <RelativeLayout
                LayoutWidth = "132"
                LayoutHeight = "132"
                LayoutAlignParentRight = "true"
                LayoutRightMargin = "-6"
                LayoutTopMargin = "6">
                <!-- 分割线 -->
                <ImageActor
                    LayoutLeftMargin = "4"
                    LayoutWidth = "2"
                    LayoutHeight = "56"
                    LayoutCenterVertical = "true"
                    Background = "@Drawable/card_list_vertical_line"/>
                <!--List折叠/展开 icon-->
                <ImageButton
                    Id = "@+Id/ImgListFold2_GP"
                    LayoutHeight = "132"
                    LayoutWidth = "132"
                    LayoutCenterInParent = "true"
                    DuplicateParentState = "true"
                    Drawable = "@Drawable/arrow_up_cardlist_tripView"
                    Background = "@Drawable/h64_background_normal_clear_list"/>
            </RelativeLayout>
            <!--List分割线-->
            <ImageActor
                LayoutWidth = "FillParent"
                LayoutHeight = "2"
                LayoutAlignParentBottom = "true"
                Background = "@Drawable/radio_button_line"/>
	    </RelativeLayout>
    </RelativeLayout>
</Resource>
