<? xml version = "1.0" encoding = "UTF-8" ?>
<Resource>
	<RelativeLayout
		Id = "@+Id/LayoutParent"
		LayoutWidth = "FillParent"
		LayoutHeight = "WrapContent">
	<RelativeLayout
		Id = "@+Id/RelativeLayoutMotorWay"
		LayoutWidth = "506"
		LayoutHeight = "128"
		LayoutLeftMargin = "32">
		<!--1级List 目的地-->
		<LinearLayout
			Id = "@+Id/Linear_MotorWay"
			LayoutWidth = "FillParent"
			LayoutHeight = "FillParent"
			Clickable = "true"
			Focusable = "true"
			HorizontalGravity = "Left"
			VerticalGravity = "Center"
			Orientation = "Vertical"
			Background = "@Drawable/card_right_angle_conrners_tripview_bg">
			<LinearLayout
				LayoutWidth = "FillParent"
				LayoutHeight = "126"
				DuplicateParentState = "true">
				<!--目的地图标-->
				<ImageActor
					Id = "@+Id/PointImageType3"
					LayoutWidth = "64"
					LayoutHeight = "64"
					LayoutLeftMargin = "20"
					LayoutTopMargin = "32"
					DuplicateParentState = "true"/>
				<LinearLayout
					LayoutWidth = "FillParent"
					LayoutHeight = "80"
					HorizontalGravity = "Left"
					DuplicateParentState = "true"
					LayoutLeftMargin = "16"
					LayoutTopMargin = "25"
					Orientation = "Vertical">
					<!--目的地名称-->
					<TextActor
						Id = "@+Id/TxtPointTitle3"
						Text ="@String/MX_LANG_NAVI_TRIPVIEW_ROUTE_WITH_STOPOVER_TEXT_Ferry_Line"
						Style = "@Style/cns_TripView_Title"
						LayoutWidth = "FillParent"
						LayoutHeight = "40"
						DuplicateParentState = "true"/>
					<LinearLayout
						LayoutHeight = "40"
						DuplicateParentState = "true"
						LayoutWidth = "FillParent">
						<!--目的地距离信息-->
						<TextActor
							Id = "@+Id/TxtDist2"
							Style = "@Style/cns_TripView_Title"
							LayoutWidth = "WrapContent"
							LayoutHeight = "40"
							DuplicateParentState = "true"/>
						<!--目的地距离单位-->
						<TextActor
							Id = "@+Id/TxtDistUnit"
							Style = "@Style/cns_TripView_Address"
							LayoutWidth = "WrapContent"
							LayoutHeight = "40"
							DuplicateParentState = "true"/>
					</LinearLayout>
				</LinearLayout>
			</LinearLayout>
			<!--List分割线-->
			<ImageActor
				LayoutWidth = "FillParent"
				LayoutHeight = "2"
				Background = "@Drawable/radio_button_line"/>
		</LinearLayout>
	</RelativeLayout>
	</RelativeLayout>
</Resource>
