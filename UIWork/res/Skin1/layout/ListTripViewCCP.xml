<? xml version = "1.0" encoding = "UTF-8" ?>
<Resource>
	<RelativeLayout
		Id = "@+Id/LayoutParent"
		LayoutWidth = "FillParent"
		LayoutHeight = "WrapContent">
	<RelativeLayout
		Id = "@+Id/RelativeLayoutCCP"
		LayoutWidth = "506"
		LayoutHeight = "128"
		LayoutLeftMargin = "32">
		<!--1级List 自车-->
		<RelativeLayout
			Id = "@+Id/LinearCCP"
			LayoutWidth = "FillParent"
			LayoutHeight = "FillParent"
			Clickable = "true"
			Focusable = "true"
			HorizontalGravity = "Left"
			VerticalGravity = "Center">
			<LinearLayout
				Id = "@+Id/LinearCCPWidth"
				LayoutWidth = "392"
				LayoutHeight = "FillParent"
				HorizontalGravity = "Left"
				VerticalGravity = "Center"
				DuplicateParentState = "true"
				Background = "@Drawable/card_right_angle_conrners_tripview_bg">
				<!--CCP图标-->
				<ImageActor
					LayoutWidth = "64"
					LayoutHeight = "64"
					LayoutLeftMargin = "20"
					LayoutCenterVertical = "true"
					DuplicateParentState = "true"
					Background = "@Drawable/current_car_position_m_c"/>
				<!--CCP名称-->
				<TextActor
					LayoutCenterVertical = "true"
					LayoutLeftMargin = "16"
					Style = "@Style/cns_TripView_Title"
					LayoutWidth = "262"
					LayoutHeight = "40"
					Text = "@String/MX_LANG_NAVI_ADD_MAIN_TEXT_Add_Current_Position"
					DuplicateParentState = "true"/>
			</LinearLayout>
			<RelativeLayout
				LayoutWidth = "150"
				LayoutHeight = "112"
				LayoutTopMargin = "8"
				LayoutAlignParentRight= "true"
				LayoutRightMargin = "-28">
				<!-- 分割线 -->
				<ImageActor
					LayoutLeftMargin = "8"
					LayoutWidth = "2"
					LayoutHeight = "56"
					LayoutCenterVertical = "true"
					Background = "@Drawable/card_list_vertical_line"/>
				<!--List折叠/展开 icon-->
				<ImageButton
					Id = "@+Id/ImgListFold1"
					LayoutHeight = "112"
					LayoutWidth = "112"
					LayoutLeftMargin = "10"
					LayoutCenterVertical = "true"
					DuplicateParentState = "true"
					Background = "@Drawable/h64_background_normal_clear_list"
					Drawable = "@Drawable/arrow_up_cardlist_tripView"/>
			</RelativeLayout>
		</RelativeLayout>
		<!--List分割线-->
		<ImageActor
			LayoutWidth = "FillParent"
			LayoutHeight = "2"
			LayoutAlignParentBottom = "true"
			Background = "@Drawable/radio_button_line"/>
	</RelativeLayout>
	</RelativeLayout>
</Resource>
