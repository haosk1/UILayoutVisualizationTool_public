<?xml version="1.0" encoding="UTF-8" ?>
<Resource>
    <RelativeLayout
        Id="@+Id/BtnInfo"
        LayoutWidth="FillParent"
        LayoutHeight="168"
        Clickable="true"
        Focusable="true">
        <RelativeLayout
            Id="@+Id/LayoutInfo"
            LayoutWidth="FillParent"
            LayoutHeight="164"
            LayoutTopMargin = "1"
            Background="@Drawable/bg_prox"
            ClickDataCollection="BTN_Search_Result"
            Clickable="false"
            Focusable="true">
            <RelativeLayout
                Id="@+Id/ListMain"
                LayoutWidth="506"
                LayoutHeight="144"
                LayoutTopMargin="8"
                LayoutLeftMargin="12"
                Clickable="true"
                Background="@Drawable/bg_prox_search_result">
                <!-- 后台功能重要度显示工具 -->
                <TextActor
                    Id="@+Id/Debug_Importance"
                    LayoutWidth="WrapContent"
                    LayoutHeight="WrapContent"
                    LayoutCenterVertical="true"
                    LayoutLeftMargin="103"
                    TextFont="@Font/UIFont46_UIDigit_M"
                    TextColor="@Color/Color_FF0000"
                    Visible="Gone" />
                <TextActor
                    Id="@+Id/Icon_ABCDE"
                    LayoutWidth="60"
                    LayoutHeight="60"
                    LayoutTopMargin="44"
                    LayoutLeftMargin="29"
                    TextFont="@Font/UIFont40_UIDigit_M"
                    TextColor="@Color/ListFavColor"
                    Text="1"
                    DuplicateParentState="true" />
                <ImageActor
                    Id="@+Id/Icon_VWMark"
                    LayoutWidth="31"
                    LayoutHeight="31"
                    LayoutLeftMargin="44"
                    Visible="Gone" />
                <!--三排信息部分-->
                <LinearLayout
                    Id="@+Id/ListInfoLayout"
                    LayoutWidth="388"
                    LayoutHeight="FillParent"
                    LayoutLeftMargin="106"
                    Orientation="Vertical"
                    HorizontalGravity="Left"
                    DuplicateParentState="true">
                    <!--第一排信息-->
                    <LinearLayout
                        Id="@+Id/ListFirstRow"
                        LayoutWidth="FillParent"
                        LayoutHeight="40"
                        LayoutTopMargin="30"
                        Orientation="Horizontal"
                        HorizontalGravity="Left"
                        DuplicateParentState="true">
                        <!--检索结果-->
                        <TextActor
                            Id="@+Id/ListMainBody"
                            TextFont="@Font/UIFont34_Text_R"
                            TextColor="@Color/ListFavColor"
                            HorizontalGravity="Left"
                            LayoutWidth="FillParent"
                            LayoutHeight="FillParent"
                            DuplicateParentState="true"
                            ShowEllipsis="true" />
                    </LinearLayout>
                    <!--第二排信息-->
                    <LinearLayout
                        Id="@+Id/ListSecondBody"
                        LayoutTopMargin="14"
                        LayoutWidth="FillParent"
                        LayoutHeight="34"
                         LayoutLeftMargin = "-1"
                        Orientation="Horizontal"
                        HorizontalGravity="Left"
                        DuplicateParentState="true">
                        <ImageActor
                            LayoutTopMargin="0"
                            Id="@+Id/Icon_pos"
                            LayoutHeight="33"
                            LayoutWidth="20"
                            DuplicateParentState="true"
                            Visible="Gone" />
                        <!--距离-->
                        <TextActor
                            Id="@+Id/ListDistance"
                            LayoutWidth="WrapContent"
                            LayoutHeight="FillParent"
                            DuplicateParentState="true"
                            ShowEllipsis="true"
                            HorizontalGravity="Left"
                            TextFont="@Font/UIFont28_Text_L"
                            TextColor="@Color/ListFavColor" />
                        <ImageActor
                            Id="@+Id/ListAddressPoint"
                            HorizontalGravity="Left"
                            LayoutWidth="5"
                            LayoutHeight="6"
                            LayoutTopMargin="14"
                            LayoutRightMargin="4"
                            LayoutLeftMargin="9"
                            DuplicateParentState="true"
                            Drawable="@Drawable/icon_separation_symbols_list" />
                        <!--地址-->
                        <TextActor
                            Id="@+Id/ListAddress"
                            TextFont="@Font/UIFont28_Text_L"
                            TextColor="@Color/ListFavColor"
                            LayoutWidth="WrapContent"
                            LayoutHeight="FillParent"
                            LayoutLeftMargin="5"
                            HorizontalGravity="Left"
                            DuplicateParentState="true"
                            SingleLine="true"
                            ShowEllipsis="true" />
                    </LinearLayout>
                </LinearLayout>
            </RelativeLayout>
            <LinearLayout
				LayoutWidth = "FillParent"
				LayoutHeight = "FillParent"
				Orientation="Horizontal"
				HorizontalGravity = "Right"
				LayoutAlignParentRight = "true"
                LayoutRightMargin = "10"> 
				<LinearLayout
					Id = "@+Id/LayoutListStopOver"
					LayoutWidth = "WrapContent"
					LayoutHeight = "FillParent"
					Orientation = "Horizontal">
					<RelativeLayout
						Id = "@+Id/LayoutAddStopover"
						LayoutWidth = "122"
						LayoutHeight = "132"
						LayoutTopMargin = "14"
						Visible = "Gone">
						<ImageActor
                                Id = "@+Id/AddStopover_VerticalImg"
								LayoutWidth="2"
								LayoutHeight="56"
								LayoutTopMargin = "38"
								LayoutLeftMargin = "4"
								Drawable="@Drawable/citylist_line"/>
							<ImageButton
								Id="@+Id/AddStopover"
								LayoutWidth="112"
								LayoutHeight="112"
								LayoutTopMargin = "10"
								LayoutLeftMargin = "10"
								ClickDataCollection = "BTN_Add_Stopover"
								Drawable="@Drawable/naviadd_view"
								Background="@Drawable/h104_background_normal_clear_list"/>
						</RelativeLayout>
					</LinearLayout>
					<ImageActor
						Id = "@+Id/ImageOccupyspace"
						LayoutWidth = "10"
						LayoutHeight = "FillParent"
						Visible = "Gone"/>
					<LinearLayout
						Id = "@+Id/LayoutListCalcu"
						LayoutWidth = "WrapContent"
						LayoutHeight = "FillParent"
						Orientation = "Horizontal">
						<RelativeLayout
							Id = "@+Id/LayoutCalculate"
							LayoutWidth = "132"
							LayoutHeight = "132"
							LayoutTopMargin = "14">
							<ImageActor
								LayoutWidth="2"
								LayoutHeight="56"
								LayoutTopMargin = "38"
								LayoutLeftMargin = "4"
								Drawable="@Drawable/citylist_line"/>
							<ImageButton
								Id="@+Id/Calculate"
								LayoutWidth="112"
								LayoutHeight="112"
								LayoutTopMargin = "10"
								LayoutLeftMargin = "10"
								ClickDataCollection = "BTN_Navigate"
								Background="@Drawable/h104_background_normal_clear_list"
								Drawable="@Drawable/icon_navi_overall_view"/>
						</RelativeLayout>
					</LinearLayout>
				</LinearLayout>
        </RelativeLayout>
    </RelativeLayout>
</Resource>
