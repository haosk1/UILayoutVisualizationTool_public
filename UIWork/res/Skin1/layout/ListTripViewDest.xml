<? xml version = "1.0" encoding = "UTF-8" ?>
<Resource>
	<RelativeLayout
		Id = "@+Id/LayoutParent"
		LayoutWidth = "FillParent"
		LayoutHeight = "WrapContent">
	<RelativeLayout
		Id = "@+Id/RelativeLayoutDest"
		LayoutWidth = "506"
		LayoutHeight = "128"
		LayoutLeftMargin = "32">
		<!--1级List 目的地-->
		<LinearLayout
			LayoutWidth = "FillParent"
			LayoutHeight = "FillParent"
			Clickable = "true"
			Focusable = "true"
			HorizontalGravity = "Left"
			VerticalGravity = "Center"
			Orientation = "Vertical"
			Background = "@Drawable/card_right_angle_conrners_tripview_bg">
			<LinearLayout
				LayoutWidth = "FillParent"
				LayoutHeight = "126"
				DuplicateParentState = "true">
				<!--目的地图标-->
				<ImageActor
					Id = "@+Id/PointImageType2"
					LayoutWidth = "64"
					LayoutHeight = "64"
					LayoutLeftMargin = "20"
					LayoutTopMargin = "32"
					Background = "@Drawable/navi_destination"
					DuplicateParentState = "true"/>
				<LinearLayout
					LayoutWidth = "FillParent"
					LayoutHeight = "80"
					HorizontalGravity = "Left"
					DuplicateParentState = "true"
					LayoutLeftMargin = "16"
					LayoutTopMargin = "24"
					Orientation = "Vertical">
					<!--目的地名称-->
					<TextActor
						Id = "@+Id/TxtPointTitle2"
						Text ="@String/MX_LANG_NAVI_TRIPVIEW_ROUTE_WITH_STOPOVER_TEXT_Ferry_Line"
						Style = "@Style/cns_TripView_Title"
						LayoutWidth = "FillParent"
						LayoutHeight = "40"
						DuplicateParentState = "true"/>
					<LinearLayout
						LayoutHeight = "40"
						DuplicateParentState = "true"
						LayoutWidth = "FillParent">
						<LinearLayout
							Id = "@+Id/Layout_ReachSoc"
							LayoutWidth = "WrapContent"
							LayoutHeight = "FillParent">
							<!--目的地电量图标-->
							<ImageActor
								Id = "@+Id/Icon_DestReachsoc"
								HorizontalGravity = "Left"
								LayoutWidth = "26"
								LayoutHeight = "FillParent"
								LayoutCenterVertical = "true"
								Background = "@Drawable/power_48_tripview"/>
							<TextActor
								Id = "@+Id/TxtReachSoc_Dest"
								LayoutWidth = "WrapContent"
								LayoutHeight = "FillParent"
								LayoutRightMargin = "10"
								Style = "@Style/cns_TripView_Address"
								DuplicateParentState = "true"/>
						</LinearLayout>
						<!--目的地距离信息-->
						<TextActor
							Id = "@+Id/TxtDist"
							Style = "@Style/cns_TripView_Title"
							LayoutWidth = "WrapContent"
							LayoutHeight = "40"
							DuplicateParentState = "true"/>
						<!--目的地距离单位-->
						<TextActor
							Id = "@+Id/TxtDistUnit_Dest"
							Style = "@Style/cns_TripView_Address"
							LayoutWidth = "WrapContent"
							LayoutHeight = "40"
							DuplicateParentState = "true"/>
						<!--到达时间-->
						<TextActor
							Id = "@+Id/TxtArrivalTime"
							Style = "@Style/cns_TripView_Title"
							LayoutLeftMargin = "21"
							LayoutWidth = "WrapContent"
							LayoutHeight = "40"
							DuplicateParentState = "true"/>
						<!--到达文言-->
						<TextActor
							Style = "@Style/cns_TripView_Address"
							LayoutWidth = "WrapContent"
							Text = "@String/MX_LANG_NAVI_MAP_CCP_VIEW_WITH_GUIDANCE_OPERATE_TEXT_Route_Info_Arrival"
							LayoutHeight = "40"
							DuplicateParentState = "true"/>
						<!--地址详情-->
						<TextActor
							Id = "@+Id/TxtDetail2"
							Style = "@Style/cns_TripView_Address"
							LayoutWidth = "WrapContent"
							LayoutHeight = "40"
							DuplicateParentState = "true"/>
					</LinearLayout>
				</LinearLayout>
			</LinearLayout>
			<!--List分割线-->
			<ImageActor
				LayoutWidth = "FillParent"
				LayoutHeight = "2"
				Background = "@Drawable/radio_button_line"/>
		</LinearLayout>
	</RelativeLayout>
	<RelativeLayout
		Id = "@+Id/RelativeLayoutDest_MP"
		LayoutWidth = "506"
		LayoutHeight = "180"
		LayoutLeftMargin = "32">
		<!--1级List 目的地-->
		<LinearLayout
			LayoutWidth = "FillParent"
			LayoutHeight = "FillParent"
			Clickable = "true"
			Focusable = "true"
			HorizontalGravity = "Left"
			VerticalGravity = "Center"
			Orientation = "Vertical"
			Background = "@Drawable/card_right_angle_conrners_tripview_bg">
			<LinearLayout
				LayoutWidth = "FillParent"
				LayoutHeight = "178"
				DuplicateParentState = "true">
				<!--目的地图标-->
				<ImageActor
					Id = "@+Id/PointImageType2_MP"
					LayoutWidth = "64"
					LayoutHeight = "64"
					LayoutLeftMargin = "20"
					LayoutTopMargin = "58"
					Background = "@Drawable/navi_destination"
					DuplicateParentState = "true"/>
				<LinearLayout
					LayoutWidth = "FillParent"
					LayoutHeight = "146"
					HorizontalGravity = "Left"
					DuplicateParentState = "true"
					LayoutLeftMargin = "14"
					LayoutTopMargin = "24"
					Orientation = "Vertical">
					<LinearLayout
						LayoutHeight = "45"
						LayoutTopMargin = "-6"
						LayoutLeftMargin = "1"
						DuplicateParentState = "true"
						LayoutWidth = "FillParent">
						<!--目的地名称-->
						<TextActor
							Id = "@+Id/TxtPointTitle2_MP"
							Text ="@String/MX_LANG_NAVI_TRIPVIEW_ROUTE_WITH_STOPOVER_TEXT_Ferry_Line"
							Style = "@Style/cns_TripView_Title"
							LayoutWidth = "FillParent"
							LayoutHeight = "45"
							DuplicateParentState = "true"/>
						</LinearLayout>
						<LinearLayout
							LayoutHeight = "52"
							DuplicateParentState = "true"
							LayoutTopMargin = "3" 
							LayoutLeftMargin = "1"
							LayoutWidth = "FillParent">
							<!--目的地距离信息-->
							<TextActor
								Id = "@+Id/TxtDist_MP"
								Style = "@Style/cns_TripView_Title"
								LayoutWidth = "WrapContent"
								LayoutHeight = "FillParent"
								DuplicateParentState = "true"/>
							<!--目的地距离单位-->
							<TextActor
								Id = "@+Id/TxtDistUnit_Dest_MP"
								Style = "@Style/cns_TripView_Address"
								LayoutWidth = "WrapContent"
								LayoutHeight = "FillParent"
								DuplicateParentState = "true"/>
							<!--到达时间-->
							<TextActor
								Id = "@+Id/TxtArrivalTime_MP"
								Style = "@Style/cns_TripView_Title"
								LayoutLeftMargin = "22"
								DuplicateParentState = "true"
								LayoutWidth = "WrapContent"
								LayoutHeight = "FillParent"/>
						<!--到达文言-->
							<TextActor
								Style = "@Style/cns_TripView_Address"
								LayoutWidth = "WrapContent"
								LayoutLeftMargin = "10"
								DuplicateParentState = "true"
								Text = "@String/MX_LANG_NAVI_MAP_CCP_VIEW_WITH_GUIDANCE_OPERATE_TEXT_Route_Info_Arrival"
								LayoutHeight = "FillParent"/>
							<!--地址详情-->
							<TextActor
								Id = "@+Id/TxtDetail2_MP"
								Style = "@Style/cns_TripView_Address"
								LayoutWidth = "WrapContent"
								DuplicateParentState = "true"
								LayoutHeight = "40"
								LayoutTopMargin = "5" />
						</LinearLayout>
					<LinearLayout
						Id = "@+Id/Layout_ReachSoc_MP"
						LayoutWidth = "WrapContent"
						LayoutHeight = "FillParent"
						LayoutTopMargin = "5" 
						LayoutLeftMargin = "4"
						DuplicateParentState = "true"
						HorizontalGravity = "Left">
						<!--目的地电量图标-->
						<ImageActor
							Id = "@+Id/Icon_DestReachsoc_MP"
							HorizontalGravity = "Left"
							LayoutWidth = "26"
							LayoutHeight = "38"
							LayoutLeftMargin = "9"
							LayoutCenterVertical = "true"
							DuplicateParentState = "true"
							Background = "@Drawable/power_48_tripview"/>
						<TextActor
							Id = "@+Id/TxtReachSoc_Dest_MP"
							LayoutWidth = "WrapContent"
							LayoutHeight = "38"
							LayoutLeftMargin = "21"
							HorizontalGravity = "Left"
							LayoutTopMargin = "2"
							DuplicateParentState = "true"
							Style = "@Style/cns_TripView_Address"/>
					</LinearLayout>
				</LinearLayout>
			</LinearLayout>
			<!--List分割线-->
			<ImageActor
				LayoutWidth = "FillParent"
				LayoutHeight = "2"
				Background = "@Drawable/radio_button_line"/>
		</LinearLayout>
	</RelativeLayout>
	</RelativeLayout>
</Resource>
