<?xml version="1.0" encoding="UTF-8" ?>
<Resource>
    <RelativeLayout
        Id="@Id/PreDefine/LayoutParent"
        LayoutHeight="FillParent"
        LayoutWidth="FillParent">
        <MXUIScreenRelativeLayout
            Id="@Id/PreDefine/MXUIScreenRelativeLayout"
            LayoutAlignParentRight="true"
            LayoutWidth="1407"
            LayoutHeight="FillParent"
            Clickable="false">
            <!--界面拖拽元器件-->
            <RelativeLayout
                LayoutTopMargin="251"
                LayoutHeight="716"
                LayoutWidth="501"
                Visible="Visible">
                <UIDropDownSlideActor
                    Id="@Id/PreDefine/UIDropDownSlideActor"
                    LayoutHeight="716"
                    LayoutWidth="501"
                    Clickable="true">
                    <!--切图不符合条件，利用现有资源手动调整显示位置，以保证显示时是上方为直角，可以和TBT完美重合-->
                    <ImageActor
                        LayoutHeight="750"
                        LayoutWidth="FillParent"
                        LayoutTopMargin="-33"
                        Background="@Drawable/card_bottom_bg_prox" />
                    <!--分割线-->
                    <ImageActor
                        LayoutHeight="2"
                        LayoutWidth="446"
                        LayoutLeftMargin="27"
                        LayoutTopMargin="15"
                        Background="@Drawable/radio_button_line" />
                    <LinearLayout
                        Id="@Id/PreDefine/Relativelayout_Main"
                        LayoutHeight="658"
                        LayoutTopMargin="16"
                        LayoutWidth="FillParent"
                        Orientation="Vertical">
                        <RelativeLayout
                            Id="@+Id/LayoutTrafficEvent"
                            LayoutWidth="FillParent"
                            LayoutHeight="118"
                            DuplicateParentState="true">
                            <RelativeLayout
                                Id="@+Id/BtnTrafficEvent"
                                LayoutWidth="446"
                                LayoutHeight="117"
                                LayoutCenterHorizontal="true"
                                DuplicateParentState="true"
                                Background="@Drawable/card_right_angle_conrners_tripview_bg"
                                Clickable="true">
                                <TextActor
                                    Id="@+Id/TrafficEventNum"
                                    LayoutWidth="281"
                                    LayoutHeight="69"
                                    LayoutTopMargin="26"
                                    LayoutLeftMargin="26"
                                    HorizontalGravity="Left"
                                    DuplicateParentState="true"
                                    VerticalGravity="Top"
                                    PaddingTop="2"
                                    TextFont="@Font/UIFont34_Text_R"
                                    TextColor="@Color/DayBlack_NightWhite"
                                    Text="@String/MX_LANG_NAVI_TRIPVIEW_ROUTE_NO_STOPOVER_TEXT_Traffic_Event"
                                    SingleLine="false"
                                    LineIntervalMultiFactor="-0.04" />
                                <ImageActor
                                    Id="@+Id/BTN_Traffic_Event"
                                    LayoutWidth="64"
                                    LayoutHeight="64"
                                    LayoutRightMargin = "24"
                                    PaddingTop="2"
                                    LayoutAlignParentRight = "true"
                                    LayoutCenterVertical = "true"
                                    DuplicateParentState="true"
                                    Drawable="@Drawable/icon_arrow_right_cardlist"
                                    ClickDataCollection="BTN_Traffic_Event" />
                            </RelativeLayout>
                            <!--分割线-->
                            <ImageActor
                                LayoutHeight="2"
                                LayoutWidth="446"
                                LayoutCenterHorizontal="true"
                                LayoutAlignParentBottom="true"
                                Background="@Drawable/radio_button_line" />
                        </RelativeLayout>
                        <RelativeLayout
                            Id="@Id/PreDefine/ListLayout"
                            LayoutWidth="FillParent"
                            LayoutHeight="FillParent"
                            LayoutTopMargin="2"
                            Orientation="Horizontal">
                            <!--ListActor-->
                            <SmoothListActorEx
                                Id="@Id/PreDefine/SmoothListActor"
                                LayoutWidth="FillParent"
                                LayoutHeight="FillParent"
                                Orientation="Vertical"
                                VisibleSpeechs="@String/NAVI_Scroll_Bar_VisibleSpeechs_With_TurnPages_And_Silde_01" />
                            <!--ScrollBar-->
                            <SmoothListScrollBar
                                Id="@Id/PreDefine/SmoothListScrollBar"
                                LayoutWidth="26"
                                LayoutHeight="FillParent"
                                LayoutTopMargin="9"
                                LayoutBottomMargin="-26"
                                LayoutRightMargin="14"
                                LayoutAlignParentRight="true"
                                ScrollBarLayout="@Layout/CActorScrollbarNewThirty"
                                ClickDataCollection="Scroll_Bar" />
                        </RelativeLayout>
                    </LinearLayout>
                    <!--路线计算中Layout-->
                    <RelativeLayout
                        Id="@Id/PreDefine/LayoutLoading"
                        LayoutWidth="FillParent"
                        LayoutHeight="FillParent"
                        Visible="Gone">
                        <AutomaticImageActor
                            Id = "@+Id/LoadingImage"
                            LayoutWidth="69"
                            LayoutHeight="69"
                            LayoutCenterHorizontal="true"
                            LayoutTopMargin="288"
                            ImageChangeTimer="83"
                            Cyclable="true"
                            Drawable="@Drawable/loading_anim_seq_l"
                            Visible="Gone" />
                        <AutomaticImageActor
                            Id = "@+Id/LoadingImage_Color"
                            LayoutWidth="69"
                            LayoutHeight="69"
                            LayoutCenterHorizontal="true"
                            LayoutTopMargin="288"
                            ImageChangeTimer="83"
                            Cyclable="true"
                            Drawable="@Drawable/loading_anim_seq_l_f_c_color"
                            Visible="Gone" />
                        <TextActor
                            LayoutWidth="WrapContent"
                            LayoutHeight="51"
                            LayoutTopMargin="386"
                            LayoutCenterHorizontal="true"
                            TextColor="@Color/DayBlack_NightWhite"
                            TextFont="@Font/UIFont46_Text_L"
                            Text="@String/MX_LANG_PO_NAVI_ROUTE_CALCULATING_TEXT_ROUTE_CALCULATING" />
                    </RelativeLayout>
                </UIDropDownSlideActor>
            </RelativeLayout>
            <!--界面拖拽bar 一条TBT情况-->
            <RelativeLayout
                Id="@Id/PreDefine/LayoutScrollBar"
                LayoutWidth="FillParent"
                LayoutHeight="FillParent">
                <DropDownScrollBar
                    Id="@Id/PreDefine/DropDownScrollBar"
                    LayoutTopMargin="248"
                    LayoutHeight="716"
                    LayoutWidth="501"
                    ScrollBarLayout="@Layout/CActorDropDownScrollBar"
                    Visible="Gone" />
            </RelativeLayout>
        </MXUIScreenRelativeLayout>
        <!--1558宽度元器件居中-->
        <RelativeLayout
            Id="@Id/PreDefine/CenterLayoutParent"
            LayoutHeight="FillParent"
            LayoutWidth="1335"
            LayoutAlignParentRight="true"
            Clickable="false">
            <!--回自车 hero btn -->
            <RelativeLayout
                Id="@Id/PreDefine/ReturnCCP"
                LayoutWidth="WrapContent"
                LayoutHeight="147"
                LayoutCenterHorizontal="true"
                LayoutAlignParentBottom="true"
                LayoutBottomMargin="98"
                Background="@Drawable/background_104h_hero_prox_day_shadows">
                <RelativeLayout
                    Id="@Id/PreDefine/BtnReturnCCP"
                    LayoutWidth="WrapContent"
                    LayoutHeight="117"
                    LayoutLeftMargin="15"
                    LayoutRightMargin="15"
                    LayoutCenterInParent="true"
                    Clickable="true"
                    VisibleSpeechs="@String/MX_LANG_NAVI_TRIPVIEW_ROUTE_NO_STOPOVER_VisibleSpeechs_01">
                    <LinearLayout
                        LayoutWidth="WrapContent"
                        LayoutHeight="96"
                        LayoutLeftMargin="10"
                        LayoutRightMargin="10"
                        LayoutCenterInParent="true"
                        DuplicateParentState="true"
                        Background="@Drawable/Gradient_background_104h_hero_prox">
                        <ImageActor
                            LayoutLeftMargin="24"
                            LayoutCenterVertical="true"
                            DuplicateParentState="true"
                            Drawable="@Drawable/back_to_destination_day" />
                        <!--占位-->
                        <ImageActor
                            Id="@Id/PreDefine/SpaceBefore"
                            LayoutWidth="2"
                            LayoutHeight="FillParent"
                            Visible="Gone" />
                        <TextActor
                            Id="@Id/PreDefine/TxtReturnCCP"
                            LayoutHeight="FillParent"
                            LayoutWidth="WrapContent"
                            LayoutLeftMargin="7"
                            LayoutRightMargin="40"
                            PaddingTop="3"
                            DuplicateParentState="true"
                            Text="@String/MX_LANG_NAVI_MAP_CCP_VIEW_PNAV2_TEXT_Back_to_CCP"
                            TextFont="@Font/BtnFont40_N_TextL_P_TextM_C_TextM_D_TextL"
                            TextColor="@Color/HeroBtnText_RGB" />
                        <!--占位-->
                        <ImageActor
                            Id="@Id/PreDefine/SpaceAfter"
                            LayoutWidth="2"
                            LayoutHeight="FillParent"
                            Visible="Gone" />
                    </LinearLayout>
                </RelativeLayout>
            </RelativeLayout>
            <!--车线-->
            <LinearLayout
                Id="@Id/PreDefine/LaneLayout"
                LayoutHeight="164"
                LayoutTopMargin="78"
                LayoutCenterHorizontal="true"
                Background="@Drawable/background_020_m_lane_info_box"
                Visible="Invisible">
                <RelativeLayout
                    LayoutWidth="WrapContent"
                    LayoutHeight="96"
                    LayoutLeftMargin = "34"
                    LayoutRightMargin = "34"
                    LayoutTopMargin="34">
                    <!--推荐车线背景帧动画-->
                    <LaneAutomaticImageActor
                        Id="@Id/PreDefine/LaneAutomatic"
                        LayoutHeight="FillParent"
                        ImageChangeTimer="38"
                        Cyclable="true"
                        Drawable="@Drawable/background_recommend_lane"
                        Visible="Gone" />
                    <LaneInfo
                        Id="@Id/PreDefine/Lane"
                        LayoutHeight="FillParent"
                        LaneRecommended="@Drawable/DrawLevel_RecommendLane"
                        LaneAvailable="@Drawable/DrawLevel_AvailableLane"
                        LaneDisable="@Drawable/DrawLevel_DisableLane"
                        LaneOmit="@Drawable/more"
                        LaneTG="@Drawable/DrawLevel_All_SpecialLane"
                        LaneMiddleImg="@Drawable/background_020_m_lane_info_box_line"
                        LanePaddingLeft="8"
                        LanePaddingRight="8"
                        LanePaddingTop="16"
                        LanePaddingBottom="16"
                        LaneMaxCnt="6"
                        Clickable="true" />
                </RelativeLayout>
            </LinearLayout>
        </RelativeLayout>
    </RelativeLayout>
</Resource>
