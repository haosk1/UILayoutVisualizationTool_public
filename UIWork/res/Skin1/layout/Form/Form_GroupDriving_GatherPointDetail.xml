<?xml version="1.0" encoding="UTF-8" ?>
<Resource>
    <RelativeLayout
        Id="@Id/PreDefine/LayoutParent"
        LayoutWidth="FillParent"
        LayoutHeight="FillParent">
        <TransparencyActor
            Id="@+Id/TransparencyScreen"
            LayoutHeight="FillParent"
            LayoutWidth="FillParent" />
        <Function
            Id="@Id/PreDefine/Fragment_GroupDriving_GatherPointDetail"
            LayoutHeight="FillParent"
            LayoutWidth="FillParent" />
        <Function
            Id="@Id/PreDefine/Fragment_SearchMain"
            LayoutHeight="FillParent"
            LayoutWidth="FillParent" />
        <!--HMI界面布局部分，此元器件LayoutWidth表示半屏尺寸-->
        <MXUIScreenRelativeLayout
            Id="@Id/PreDefine/MXUIScreenRelativeLayout"
            LayoutAlignParentRight="true"
            LayoutWidth="1424"
            LayoutHeight="FillParent">
            <!--SearchBar-->
            <LinearLayout
                Id="@Id/PreDefine/SearchBar"
                LayoutHeight="98"
                LayoutWidth="476"
                LayoutTopMargin="112"
                LayoutLeftMargin="16"
                Background="@Drawable/search_bg"
                Clickable="true">
                <Button
                    Id="@Id/PreDefine/BTN_Search"
                    LayoutHeight="FillParent"
                    LayoutWidth="FillParent"
                    Text="@String/MX_LANG_NAVI_MAP_CCP_VIEW_PNAV1_TEXT_HInt_Text1"
                    TextFont="@Font/UIFont34_Text_L"
                    TextColor="@Color/DayBlack_NightWhite_Grey"
                    PaddingLeft="48"
                    PaddingTop="4"
                    HorizontalGravity="Left"
                    DuplicateParentState="true"
                    ClickDataCollection="BTN_Search" />
            </LinearLayout>
            <!-- 组队绑定画页布局 -->
            <RelativeLayout
                Id="@Id/PreDefine/LayoutMain"
                LayoutHeight="FillParent"
                LayoutWidth="FillParent"
                LayoutTopMargin="348">
                <!-- 背景图片 -->
                <RelativeLayout
                    Id="@+Id/LayoutBG"
                    LayoutHeight="620"
                    LayoutWidth="584"
                    Background="@Drawable/card_bg_day_and_night"
                    Clickable="true">
                    <RelativeLayout
                        Id="@+Id/FullScreen"
                        LayoutWidth="552"
                        LayoutHeight="588"
                        LayoutLeftMargin="16"
                        LayoutTopMargin="16">
                        <!-- 返回按钮 -->
                        <RelativeLayout
                            LayoutWidth="152"
                            LayoutHeight="152"
                            LayoutTopMargin="-4"
                            LayoutLeftMargin="-4">
                            <ImageButton
                                Id="@+Id/BtnBack"
                                LayoutWidth="116"
                                LayoutHeight="116"
                                LayoutCenterInParent="true"
                                Clickable="true"
                                Drawable="@Drawable/back_s"
                                Background="@Drawable/btn_h84_background_normal_prox"
                                BGHorizontalGravity="Center"
                                BGVerticalGravity="Center"
                                ClickDataCollection="BTN_Back"
                                VisibleSpeechs="@String/MX_LANG_NAVI_GROUPDRIVING_FLEET_MAIN_MEMBER_DETAIL_VisibleSpeechs_01" />
                        </RelativeLayout>
                        <!-- 集结码 -->
                        <LinearLayout
                            LayoutHeight="52"
                            LayoutWidth="378"
                            LayoutTopMargin="48"
                            LayoutLeftMargin="144"
                            VerticalGravity="Center">
                            <TextActor
                                Id="@+Id/FleetID"
                                LayoutHeight="WrapContent"
                                LayoutWidth="WrapContent"
                                PaddingTop="3"
                                TextFont="@Font/UIFont40_Text_M"
                                TextColor="@Color/BtnFavoriteColor"
                                Text="@String/MX_LANG_NAVI_GROUPDRIVING_FLEET_MAIN_NO_GATHERING_POINT_TEXT_Group_ID" />
                        </LinearLayout>
                        <RelativeLayout
                            Id="@+Id/Layout_MemerDetail"
                            LayoutHeight="FillParent"
                            LayoutWidth="FillParent">
                            <!--主体-->
                            <RelativeLayout
                                Id="@+Id/MainInfo"
                                LayoutWidth="FillParent"
                                LayoutHeight="326"
                                LayoutTopMargin="114">
                                <UIScrollViewEx
                                    Id="@Id/PreDefine/ScrollViewEx"
                                    LayoutWidth="FillParent"
                                    LayoutHeight="FillParent"
                                    ShowsIndicator="false"
                                    Orientation="Vertical"
                                    BounceVertical="true"
                                    BounceHorizontal="false">
                                    <LinearLayout
                                        Id="@+Id/LinearInfoMain"
                                        LayoutWidth="FillParent"
                                        LayoutHeight="WrapContent"
                                        Orientation="Vertical">
                                        <RelativeLayout
                                            LayoutWidth="FillParent"
                                            LayoutHeight="116">
                                            <TextActor
                                                Id="@+Id/GatherPoint_Name"
                                                LayoutWidth="319"
                                                LayoutHeight="WrapContent"
                                                LayoutLeftMargin="30"
                                                LayoutTopMargin="52"
                                                HorizontalGravity="Left"
                                                VerticalGravity="Center"
                                                TextFont="@Font/UIFont34_Text_R"
                                                TextColor="@Color/DayBlack_NightWhite"
                                                Text="Name"
                                                SingleLine="true" />
                                            <!--分割线-->
                                            <ImageActor
                                                Id="@+Id/Parting_Line"
                                                LayoutWidth= "2"
											    LayoutHeight= "56"
                                                LayoutTopMargin="42"
                                                LayoutLeftMargin="426"
                                                Drawable="@Drawable/citylist_line_dayAndnight"
                                                DuplicateParentState="true" />
                                            <!--集结点收藏-->
                                            <RelativeLayout
                                                LayoutWidth="152"
                                                LayoutHeight="152"
                                                LayoutTopMargin="25"
                                                LayoutLeftMargin="410">
                                                <ImageButton
                                                    Id="@+Id/Btn_GroupPoint"
                                                    LayoutWidth="116"
                                                    LayoutHeight="116"
                                                    LayoutCenterInParent="true"
                                                    Clickable="true"
                                                    Drawable="@Drawable/navi_group_s_day_and_night_48"
                                                    Background="@Drawable/h64_background_normal"
                                                    BGHorizontalGravity="Center"
                                                    BGVerticalGravity="Center"
                                                    ClickDataCollection="BTN_Gathering_Point" />
                                            </RelativeLayout>
                                            <RelativeLayout
                                                LayoutHeight="FillParent"
                                                LayoutWidth="FillParent">
                                                <!--集结点加载动画-->
                                                <AutomaticImageActor
                                                    Id="@+Id/FormFlashIcon"
                                                    LayoutHeight="48"
                                                    LayoutWidth="48"
                                                    LayoutTopMargin="47"
                                                    VerticalGravity="Center"
                                                    HorizontalGravity="Center"
                                                    LayoutLeftMargin="457"
                                                    Drawable="@Drawable/loading_anim_seq_s_LayerList"
                                                    ImageChangeTimer="83"
                                                    Visible="Gone"
                                                    Cyclable="true" />
                                            </RelativeLayout>
                                        </RelativeLayout>
                                        <!-- 地址 -->
                                        <LinearLayout
                                            LayoutWidth="422"
                                            LayoutHeight="WrapContent"
                                            LayoutTopMargin="42"
                                            LayoutLeftMargin="30"
                                            HorizontalGravity="Left"
                                            LayoutAlignParentLeft="true">
                                            <TextActor
                                                Id="@+Id/GatherPoint_Address"
                                                LayoutWidth="FillParent"
                                                LayoutHeight="WrapContent"
                                                HorizontalGravity="Left"
                                                VerticalGravity="Top"
                                                TextFont="@Font/UIFont34_Text_L"
                                                TextColor="@Color/DayBlack_NightWhite"
                                                Text="Address"
                                                SingleLine="false"
                                                LineIntervalAddFactor="-13"
                                                LineIntervalMultiFactor="0.28"
                                                WordWrapped="true" />
                                        </LinearLayout>
                                    </LinearLayout>
                                </UIScrollViewEx>
                                <ScrollViewBar
                                    Id="@Id/PreDefine/ScrollViewBar"
                                    LayoutHeight="394"
                                    LayoutWidth="6"
                                    LayoutTopMargin="3"
                                    LayoutAlignParentRight="true"
                                    LayoutRightMargin="7"
                                    ScrollBarLayout="@Layout/CActorScrollBarNew" />
                            </RelativeLayout>
                        </RelativeLayout>
                        <!--底部按钮-->
                        <LinearLayout
                            Id="@+Id/LayoutBottomInfo_NoGuidance"
                            LayoutAlignParentBottom="true"
                            LayoutWidth="FillParent"
                            LayoutHeight="152"
                            LayoutBottomMargin="-4"
                            LayoutVerticalGravity="Center"
                            VerticalGravity="Center">
                            <!--周边检索-->
                            <ImageButton
                                Id="@+Id/Btn_SearchNearby"
                                LayoutWidth="116"
                                LayoutHeight="116"
                                LayoutLeftMargin="14"
                                ClickDataCollection="BTN_Search_Around"
                                Background="@Drawable/btn_h84_background_normal_prox"
                                VisibleSpeechs="@String/MX_LANG_NAVI_SEARCH_RESULT_DETAIL_WITHOUT_GUIDANCE_VisibleSpeechs_05"
                                Drawable="@Drawable/icon_search_around_s" />
                            <!--快捷导航-->
                            <ImageButton
                                Id="@+Id/Btn_Navigate"
                                LayoutWidth="116"
                                LayoutHeight="116"
                                LayoutLeftMargin="-2"
                                ClickDataCollection="Btn_Navigate"
                                Drawable="@Drawable/navi_navigation_s"
                                Background="@Drawable/btn_h84_background_normal_prox"
                                VisibleSpeechs="@String/MX_LANG_NAVI_GROUPDRIVING_FLEET_MAIN_GATHERING_POINT_DETAIL_WITHOUT_GUIDANCE_VisibleSpeechs_03" />
                            <!--算路-->
                            <RelativeLayout
                                LayoutWidth="332"
                                LayoutHeight="152"
                                LayoutLeftMargin="-20"
                                Background="@Drawable/background_84h_hero_prox_day_shadows">
                                <RelativeLayout
                                    Id="@+Id/Btn_Calculate"
                                    LayoutWidth="296"
                                    LayoutHeight="116"
                                    LayoutCenterInParent="true"
                                    Clickable="true"
                                    ClickDataCollection="Btn_Calculate"
                                    VisibleSpeechs="@String/MX_LANG_NAVI_GROUPDRIVING_FLEET_MAIN_GATHERING_POINT_DETAIL_WITHOUT_GUIDANCE_VisibleSpeechs_04">
                                    <RelativeLayout
                                        LayoutWidth="272"
                                        LayoutHeight="92"
                                        LayoutCenterInParent="true"
                                        DuplicateParentState="true"
                                        Background="@Drawable/Route_background_84h_hero_prox">
                                        <TextActor
                                            LayoutHeight="40"
                                            LayoutWidth="200"
                                            LayoutLeftMargin="35"
                                            LayoutTopMargin="26"
                                            PaddingLeft="3"
                                            PaddingTop="5"
                                            LayoutRightMargin="27"
                                            DuplicateParentState="true"
                                            Text="@String/MX_LANG_NAVI_MAP_POI_PICKVIEW_WITHOUT_GUIDANCE_TEXT_Calculate"
                                            TextFont="@Font/BtnFont34_N_TextL_P_TextM_C_TextM_D_TextL"
                                            TextColor="@Color/Color_000000" />
                                    </RelativeLayout>
                                </RelativeLayout>
                            </RelativeLayout>
                        </LinearLayout>
                        <LinearLayout
                            Id="@+Id/LayoutBottomInfo_WithGuidance"
                            LayoutAlignParentBottom="true"
                            LayoutWidth="FillParent"
                            LayoutHeight="152"
                            LayoutBottomMargin="-4"
                            LayoutVerticalGravity="Center"
                            VerticalGravity="Center"
                            Visible="Gone">
                            <!--添加途径地-->
                            <RelativeLayout
                                LayoutWidth="120"
                                LayoutHeight="120"
                                LayoutLeftMargin = "12">
                                <ImageButton
                                    Id="@+Id/Btn_Add_Stopover"
                                    LayoutWidth="152"
                                    LayoutHeight="152"
                                    LayoutCenterInParent = "true"
                                    Drawable = "@Drawable/add_48"
                                    Background="@Drawable/btn_h84_background_normal_prox"
                                    DuplicateParentState="true"
                                    BGHorizontalGravity= "Center"
                                    BGVerticalGravity= "Center"
                                    LayoutLeftMargin="26"
                                    Clickable="true"
                                    ClickDataCollection="Btn_Add_Stopover"
                                    VisibleSpeechs="@String/MX_LANG_NAVI_GROUPDRIVING_FLEET_MAIN_GATHERING_POINT_DETAIL_VisibleSpeechs_03"/>
                            </RelativeLayout>
                            <!--更改目的地-->
                            <RelativeLayout
                                Id="@+Id/Btn_ModifyDest"
                                LayoutWidth="410"
                                LayoutHeight="116"
                                Clickable="true"
                                LayoutLeftMargin="-3"
                                ClickDataCollection="Btn_Navigate"
                                VisibleSpeechs="@String/MX_LANG_NAVI_GROUPDRIVING_FLEET_MAIN_GATHERING_POINT_DETAIL_VisibleSpeechs_04">
                                <TextActor
                                    LayoutWidth="446"
                                    LayoutHeight="152"
                                    LayoutTopMargin="-18"
                                    LayoutBottomMargin="-18"
                                    LayoutLeftMargin="-19"
                                    LayoutRightMargin="-21"
                                    PaddingTop="5"
                                    PaddingLeft="57"
                                    PaddingRight="57"
                                    Background="@Drawable/background_84h_normal_prox"
                                    DuplicateParentState="true"
                                    TextFont=" @Font/BtnFont34_N_TextL_P_TextM_C_TextM_D_TextL"
                                    TextColor="@Color/NormalBtn_RGB"
                                    Text="@String/MX_LANG_NAVI_GROUPDRIVING_FLEET_MAIN_MEMBER_DETAIL_TEXT_Navigate1" />
                            </RelativeLayout>
                        </LinearLayout>
                    </RelativeLayout>
                </RelativeLayout>
            </RelativeLayout>
        </MXUIScreenRelativeLayout>
        <!--1558宽度元器件居中-->
        <RelativeLayout
            Id="@Id/PreDefine/CenterLayoutParent"
            LayoutHeight="FillParent"
            LayoutWidth="1314"
            LayoutAlignParentRight="true"
            Clickable="false">
            <RelativeLayout
                Id="@Id/PreDefine/ReturnCCP"
                LayoutWidth="WrapContent"
                LayoutHeight="164"
                LayoutCenterHorizontal="true"
                LayoutAlignParentBottom="true"
                LayoutBottomMargin="94"
                Background="@Drawable/background_96h_hero_prox_day_shadows">
                <RelativeLayout
                    Id="@Id/PreDefine/BtnReturnCCP"
                    LayoutWidth="WrapContent"
                    LayoutHeight="164"
                    LayoutLeftMargin="18"
                    LayoutRightMargin="18"
                    LayoutCenterInParent="true"
                    VisibleSpeechs="@String/MX_LANG_NAVI_MAP_RESTRICTION_VIEW_WITHOUT_GUIDANCE_VisibleSpeechs_02"
                    Clickable="true">
                    <LinearLayout
                        LayoutWidth="WrapContent"
                        LayoutHeight="104"
                        LayoutLeftMargin="12"
                        LayoutRightMargin="12"
                        LayoutCenterInParent="true"
                        DuplicateParentState="true"
                        Background="@Drawable/Gradient_background_96h_hero_prox">
                        <ImageActor
                            LayoutLeftMargin="28"
                            LayoutCenterVertical="true"
                            DuplicateParentState="true"
                            Drawable="@Drawable/back_to_destination_day" />
                        <!--占位-->
                        <ImageActor
                            Id="@Id/PreDefine/SpaceBefore"
                            LayoutWidth="2"
                            LayoutHeight="FillParent"
                            Visible="Gone" />
                        <TextActor
                            Id="@Id/PreDefine/TxtReturnCCP"
                            LayoutHeight="FillParent"
                            LayoutWidth="WrapContent"
                            LayoutLeftMargin="8"
                            LayoutRightMargin="47"
                            PaddingTop="4"
                            DuplicateParentState="true"
                            Text="@String/MX_LANG_NAVI_MAP_CCP_VIEW_PNAV2_TEXT_Back_to_CCP"
                            TextFont="@Font/BtnFont40_N_TextL_P_TextM_C_TextM_D_TextL"
                            TextColor="@Color/HeroBtnText_RGB" />
                        <!--占位-->
                        <ImageActor
                            Id="@Id/PreDefine/SpaceAfter"
                            LayoutWidth="2"
                            LayoutHeight="FillParent"
                            Visible="Gone" />
                    </LinearLayout>
                </RelativeLayout>
            </RelativeLayout>
            <!--车线-->
            <LinearLayout
                Id="@Id/PreDefine/LaneLayout"
                LayoutHeight="164"
                LayoutTopMargin="78"
                LayoutCenterHorizontal="true"
                Background="@Drawable/background_020_m_lane_info_box"
                Visible="Invisible">
                <RelativeLayout
                    LayoutWidth="WrapContent"
                    LayoutHeight="96"
                    LayoutLeftMargin = "34"
                    LayoutRightMargin = "34"
                    LayoutTopMargin="34">
                    <!--推荐车线背景帧动画-->
                    <LaneAutomaticImageActor
                        Id="@Id/PreDefine/LaneAutomatic"
                        LayoutHeight="FillParent"
                        ImageChangeTimer="38"
                        Cyclable="true"
                        Drawable="@Drawable/background_recommend_lane"
                        Visible="Gone" />
                    <LaneInfo
                        Id="@Id/PreDefine/Lane"
                        LayoutHeight="FillParent"
                        LaneRecommended="@Drawable/DrawLevel_RecommendLane"
                        LaneAvailable="@Drawable/DrawLevel_AvailableLane"
                        LaneDisable="@Drawable/DrawLevel_DisableLane"
                        LaneOmit="@Drawable/more"
                        LaneTG="@Drawable/DrawLevel_All_SpecialLane"
                        LaneMiddleImg="@Drawable/background_020_m_lane_info_box_line"
                        LanePaddingLeft="8"
                        LanePaddingRight="8"
                        LanePaddingTop="16"
                        LanePaddingBottom="16"
                        LaneMaxCnt="6"
                        Clickable="true" />
                </RelativeLayout>
            </LinearLayout>
        </RelativeLayout>
    </RelativeLayout>
</Resource>
