<?xml version="1.0" encoding="UTF-8" ?>
<Resource>
    <RelativeLayout
        LayoutWidth="FillParent"
        LayoutHeight="FillParent"
        Clickable="true"
        Background="@Drawable/navi_image_mask_bg"
        Id="@+Id/Relative_Main">
        <!--占位用的smartcard -->
        <ImageActor
            Id="@Id/PreDefine/ImgSmartCard"
            LayoutWidth="496"
            LayoutHeight="FillParent" />
        <RelativeLayout
            Id="@+Id/Relative_List"
            LayoutWidth="1172"
            LayoutHeight="908"
            LayoutRightOf="@Id/PreDefine/ImgSmartCard"
            Background="@Drawable/down_box_bg"
            Clickable="true"
            LayoutLeftMargin="-18"
            LayoutTopMargin="78">
            <RelativeLayout
                Id="@+Id/SuperListLayout"
                LayoutWidth="FillParent"
                LayoutHeight="FillParent">
                <SuperList
                    Id="@+Id/SuperList"
                    LayoutTopMargin="44"
                    LayoutLeftMargin="34"
					LayoutWidth="1074"
                    LayoutHeight="662"
                    VisibleSpeechs="@String/NAVI_Scroll_Bar_VisibleSpeechs_With_TurnPages_And_Silde_01" />
                <SmoothListScrollBar
                    Id="@+Id/SmoothListScrollBar"
                    LayoutHeight="642"
                    LayoutWidth="66"
                    LayoutTopMargin="64"
                    LayoutAlignParentRight="true"
                    LayoutRightMargin="34"
                    ScrollBarLayout="@Layout/CActorScrollBarNew" />
            </RelativeLayout>
            <RelativeLayout
                Id="@+Id/BottomBtn"
                LayoutWidth="FillParent"
                LayoutHeight="172"
                LayoutLeftMargin="30"
                LayoutAlignParentBottom="true"
                LayoutBottomMargin="30">
                <RelativeLayout
                    LayoutWidth="548"
                    LayoutHeight="172"
                    Background="@Drawable/background_104h_hero_prox_day_shadows">
                    <RelativeLayout
                        Id="@+Id/BTN_Reset"
                        LayoutWidth="548"
                        LayoutHeight="136"
                        LayoutCenterInParent="true"
                        Clickable="true"
                        VisibleSpeechs="@String/MX_LANG_NAVI_MAP_CHARGING_MAP_FILTER_EV_SVW_VisibleSpeechs_01">
                        <RelativeLayout
                            LayoutWidth="548"
                            LayoutHeight="176"
                            LayoutTopMargin="-18"
                            LayoutBottomMargin="-18"
                            LayoutCenterHorizontal="true"
                            DuplicateParentState="true"
                            BGHorizontalGravity="Center"
                            BGVerticalGravity="Center"
                            Background="@Drawable/background_104h_normal_view_raisedNew">
                            <!-- background_104h_normal_view_raised -->
                            <TextActor
                                LayoutHeight="56"
                                LayoutWidth="380"
                                LayoutCenterInParent="true"
                                LayoutTopMargin="39"
                                PaddingTop="2"
                                DuplicateParentState="true"
                                Text="@String/MX_LANG_NAVI_SEARCH_RESULT_CHARGING_FILTER_EV_TEXT_Reset"
                                TextFont="@Font/BtnFont40_N_TextL_P_TextM_C_TextM_D_TextL"
                                TextColor="@Color/NormalBtn_RGB" />
                        </RelativeLayout>
                    </RelativeLayout>
                </RelativeLayout>
                <RelativeLayout
                    LayoutWidth="548"
                    LayoutHeight="172"
                    LayoutLeftMargin="528"
                    Background="@Drawable/background_104h_hero_prox_day_shadows">
                    <RelativeLayout
                        Id="@+Id/BTN_Confirm"
                        LayoutWidth="492"
                        LayoutHeight="136"
                        LayoutCenterInParent="true"
                        Clickable="true"
                        VisibleSpeechs="@String/MX_LANG_NAVI_MAP_CHARGING_MAP_FILTER_EV_SVW_VisibleSpeechs_02">
                        <RelativeLayout
                            LayoutWidth="488"
                            LayoutHeight="112"
                            LayoutCenterInParent="true"
                            DuplicateParentState="true"
                            Background="@Drawable/Gradient_background_104h_hero_prox">
                            <TextActor
                                LayoutHeight="56"
                                LayoutWidth="380"
                                LayoutCenterHorizontal="true"
                                LayoutTopMargin="29"
                                PaddingLeft="3"
                                DuplicateParentState="true"
                                Text="@String/MX_LANG_NAVI_SEARCH_RESULT_CHARGING_FILTER_EV_TEXT_Confirm"
                                TextFont="@Font/BtnFont40_N_TextL_P_TextM_C_TextM_D_TextL"
                                TextColor="@Color/Color_000000" />
                        </RelativeLayout>
                    </RelativeLayout>
                </RelativeLayout>
            </RelativeLayout>
        </RelativeLayout>
    </RelativeLayout>
</Resource>
