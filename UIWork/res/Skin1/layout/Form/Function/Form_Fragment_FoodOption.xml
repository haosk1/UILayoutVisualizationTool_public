                        <RelativeLayout
                            LayoutWidth="147"
                            LayoutHeight="147"
                            Background="@Drawable/btn_h104_background_normal_prox"
                            LayoutLeftMargin="-14"
                            LayoutRightMargin="-14"
                            LayoutTopMargin="-14"
                            LayoutBottomMargin="-14"
