                    <SmoothListScrollBar
                        Id="@+Id/SmoothListScrollBar"
                        LayoutHeight="507"
                        LayoutWidth="30"
                        LayoutAlignParentRight="true"
                        LayoutRightMargin="2"
                        LayoutTopMargin="9"
                        ScrollBarLayout="@Layout/CActorScrollbarNewThirty" />
