                <TextActor
                    Id="@+Id/Tilte_Setting"
                    HorizontalGravity="Left"
                    LayoutLeftMargin="180"
                    LayoutTopMargin="72"
                    LayoutHeight="52"
                    PaddingTop="4"
                    LayoutWidth="160"
                    Text="@String/MX_LANG_NAVI_GUIDANCE_SETTING_TITLE_SETTING"
                    TextFont="@Font/UIFont40_Text_M"
                    TextColor="@Color/DayBlack_NightWhite" />
                <LinearLayout
                    LayoutHeight="WrapContent"
                    LayoutWidth="300"
                    LayoutTopMargin="170"
                    Orientation="Vertical"
                    LayoutLeftMargin="46"
                    Background="@Drawable/mixin_bg_normal_DayNight">
