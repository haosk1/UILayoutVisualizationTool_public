                            LayoutHeight="130"
                            PaddingTop="5"
                            PaddingLeft="1"
                            Background="@Drawable/menu_bg_3_Setting"
                            VerticalGravity="Center"
                            Text="@String/MX_LANG_NAVI_CHARGING_SETTING_EV_TEXT_Chargingsetting"
