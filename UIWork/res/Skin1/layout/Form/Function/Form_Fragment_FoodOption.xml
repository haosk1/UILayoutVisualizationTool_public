                    HorizontalGravity="Left"
                    LayoutLeftMargin="510"
                    LayoutTopMargin="198"
                    LayoutHeight="48"
                    LayoutWidth="610"
                    Text="@String/MX_LANG_NAVI_MAP_SETTING_EV_TEXT_Food_Option"
                    TextFont="@Font/UIFont34_Text_R"
                    TextColor="@Color/DayBlack_NightWhite" />
