                        <ImageActor
                            Id="@Id/PreDefine/BTN_Group_Driving_Blank"
                            LayoutWeight="2"
                            LayoutHeight="0"
                            LayoutWidth="300"
                            Visible="Gone" />
                    </LinearLayout>
                    <!-- Charging Btn与分割线 -->
                    <LinearLayout
