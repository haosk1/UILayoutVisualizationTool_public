                        LayoutHeight="132"
                        LayoutWidth="300"
                        LayoutTopMargin="-2"
                        Orientation="Vertical">
                        <RelativeLayout
                            LayoutWeight="130"
                            LayoutHeight="0"
                            LayoutWidth="300">
                            <Button
                                Id="@Id/PreDefine/BTN_Group_Driving_Setting"
                                LayoutWidth="300"
                                LayoutHeight="FillParent"
                                PaddingTop="3"
                                Background="@Drawable/menu_bg_2_Setting"
                                VerticalGravity="Center"
                                PaddingLeft="3"
                                Text="@String/MX_LANG_NAVI_GUIDANCE_SETTING_TEXT_Group_Driving_Setting"
                                TextFont="@Font/UIFont40_Text_R"
                                TextColor="@Color/ListFavColor"
                                IsBgAnimator = "true"
                                VisibleSpeechs="@String/MX_LANG_NAVI_MAP_SETTING_VisibleSpeechs_06"
                                ClickDataCollection="BTN_Group_Driving_Setting" />
                            <ImageActor
                                Id="@Id/PreDefine/Icon_line"
                                LayoutHeight="2"
                                LayoutWidth="300"
                                LayoutTopMargin="128"
