                        DuplicateParentState="true" />
                    <!--文言-->
                    <TextActor
                        LayoutWidth="262"
                        LayoutHeight="WrapContent"
                        LayoutTopMargin = "50"
                        LayoutHorizontalGravity = "Center"
                        PaddingRight = "28"
                        DuplicateParentState="true"
                        Text="@String/MX_LANG_NAVI_SEARCH_RESULT_CHARGING_FILTER_EV_TEXT_Reset"
                        TextFont="@Font/BtnFont40_N_TextL_P_TextM_C_TextM_D_TextL"
                        TextColor="@Color/NormalBtn_RGB" />
                </RelativeLayout>
            </RelativeLayout>
        </MXUIScreenRelativeLayout>
