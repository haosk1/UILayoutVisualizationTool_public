                    LayoutHeight="136"
                    LayoutWidth="136"
                    LayoutLeftMargin="30"
                    LayoutTopMargin="30">
                    <!--圆形BTN 热区 104*104 +16-->
                    <RelativeLayout
                        Id="@+Id/BTN_Back"
                        LayoutWidth="117"
                        LayoutHeight="117"
                        ClickDataCollection="BTN_Back"
                        VisibleSpeechs="@String/MX_LANG_NAVI_MAP_FOOD_OPTION_EV_VisibleSpeechs_01"
                        LayoutCenterInParent="true"
                        Clickable="true">
                        <!--圆形BTN背景图-->
                        <RelativeLayout
                            LayoutWidth="147"
