<?xml version="1.0" encoding="UTF-8" ?>
<Resource>
    <RelativeLayout
        Id="@Id/PreDefine/LayoutRelativeSetting"
        LayoutHeight="FillParent"
        Background="@Drawable/navi_image_mask_bg"
        LayoutWidth="FillParent"
        Clickable="true">
        <MXUIScreenRelativeLayout
            Id="@Id/PreDefine/MXUIScreenRelativeLayout"
            LayoutAlignParentRight="true"
            LayoutWidth="1407"
            LayoutHeight="FillParent">
            <RelativeLayout
                Id="@Id/PreDefine/MainLayout"
                LayoutWidth="1293"
                LayoutHeight="864"
                LayoutTopMargin="103"
                Clickable="true"
                Background="@Drawable/card_bg_day_and_night">
                <!--返回BTN-->
                <!--阴影背景图 172*172-->
                <RelativeLayout
                    LayoutHeight="147"
                    LayoutWidth="147"
                    LayoutLeftMargin="10"
                    LayoutTopMargin="10">
                    <!--圆形BTN 热区 104*104 +16-->
                    <RelativeLayout
                        Id="@Id/PreDefine/BTN_Back"
                        LayoutWidth="117"
                        LayoutHeight="117"
                        VisibleSpeechs="@String/MX_LANG_NAVI_NOTIFICATIONS_SETTING_VisibleSpeechs_01"
                        LayoutCenterInParent="true"
                        Clickable="true">
                        <!--圆形BTN背景图-->
                        <RelativeLayout
                            LayoutWidth="147"
                            LayoutHeight="147"
                            Background="@Drawable/btn_h104_background_normal_prox"
                            LayoutLeftMargin="-14"
                            LayoutRightMargin="-14"
                            LayoutTopMargin="-14"
                            LayoutBottomMargin="-14"
                            DuplicateParentState="true">
                            <ImageActor
                                Id="@Id/PreDefine/BTN_Backbackground"
                                LayoutWidth="69"
                                LayoutHeight="69"
                                LayoutLeftMargin="39"
                                LayoutTopMargin="39"
                                DuplicateParentState="true"
                                Background="@Drawable/close_favorite" />
                        </RelativeLayout>
                    </RelativeLayout>
                </RelativeLayout>
                <TextActor
                    Id="@+Id/Tilte_Setting"
                    HorizontalGravity="Left"
                    LayoutLeftMargin="154"
                    LayoutTopMargin="58"
                    LayoutHeight="51"
                    PaddingTop="5"
                    LayoutWidth="1068"
                    Text="@String/MX_LANG_NAVI_GUIDANCE_SETTING_TITLE_SETTING"
                    TextFont="@Font/UIFont46_Text_M"
                    TextColor="@Color/DayBlack_NightWhite" />
                <LinearLayout
                    LayoutHeight="WrapContent"
                    LayoutWidth="267"
                    LayoutTopMargin="147"
                    Orientation="Vertical"
                    LayoutLeftMargin="39"
                    Background="@Drawable/mixin_bg_normal_DayNight">
                    <!-- Guidance Btn -->
                    <RelativeLayout
                        LayoutHeight="111"
                        LayoutWidth="267">
                        <Button
                            Id="@Id/PreDefine/Btn_Guidance_Setting"
                            LayoutWidth="267"
                            LayoutHeight="111"
                            PaddingTop="7"
                            VerticalGravity="Center"
                            PaddingLeft="2"
                            Background="@Drawable/menu_bg_1"
                            Text="@String/MX_LANG_NAVI_GUIDANCE_SETTING_TEXT_Guidance_Setting"
                            TextFont="@Font/UIFont46_Text_R"
                            TextColor="@Color/ListFavColor"
                            VisibleSpeechs="@String/MX_LANG_NAVI_NOTIFICATIONS_SETTING_VisibleSpeechs_02"
                            ClickDataCollection="Btn_Guidance_Setting" />
                        <ImageActor
                            LayoutHeight="2"
                            LayoutWidth="267"
                            LayoutTopMargin="110"
                            Background="@Drawable/menu_line_DayNight" />
                    </RelativeLayout>
                    <!-- map Btn与分割线 -->
                    <RelativeLayout
                        LayoutHeight="111"
                        LayoutWidth="267">
                        <Button
                            Id="@Id/PreDefine/Btn_Map_Setting"
                            LayoutWidth="267"
                            LayoutHeight="111"
                            PaddingTop="7"
                            PaddingRight="1"
                            Background="@Drawable/menu_bg_2"
                            VerticalGravity="Center"
                            Text="@String/MX_LANG_NAVI_GUIDANCE_SETTING_TEXT_Map_Setting"
                            TextFont="@Font/UIFont46_Text_R"
                            TextColor="@Color/ListFavColor"
                            VisibleSpeechs="@String/MX_LANG_NAVI_NOTIFICATIONS_SETTING_VisibleSpeechs_03"
                            ClickDataCollection="Btn_Map_Setting" />
                        <ImageActor
                            LayoutHeight="2"
                            LayoutWidth="267"
                            LayoutTopMargin="110"
                            Background="@Drawable/menu_line_DayNight" />
                    </RelativeLayout>
                    <!-- Notifications Btn与分割线 -->
                    <RelativeLayout
                        LayoutHeight="111"
                        LayoutWidth="267">
                        <Button
                            Id="@Id/PreDefine/BTN_Notifications_Setting"
                            LayoutWidth="267"
                            LayoutHeight="111"
                            PaddingTop="7"
                            Background="@Drawable/menu_bg_2"
                            VerticalGravity="Center"
                            Text="@String/MX_LANG_NAVI_GUIDANCE_SETTING_TEXT_Notifications_Setting"
                            TextFont="@Font/UIFont46_Text_R"
                            TextColor="@Color/ListFavColor"
                            VisibleSpeechs="@String/MX_LANG_NAVI_NOTIFICATIONS_SETTING_VisibleSpeechs_04"
                            ClickDataCollection="BTN_Notifications_Setting" />
                        <ImageActor
                            LayoutHeight="2"
                            LayoutWidth="267"
                            LayoutTopMargin="110"
                            Background="@Drawable/menu_line_DayNight" />
                    </RelativeLayout>
                    <!-- Audio Btn与分割线 -->
                    <LinearLayout
                        LayoutHeight="113"
                        LayoutWidth="267"
                        Orientation="Vertical">
                        <RelativeLayout
                            LayoutHeight="0"
                            LayoutWeight="130"
                            LayoutWidth="267">
                            <Button
                                Id="@Id/PreDefine/BTN_Audio_Setup_Setting"
                                LayoutWidth="267"
                                LayoutHeight="FillParent"
                                PaddingTop="7"
                                PaddingLeft="-1"
                                Background="@Drawable/menu_bg_2"
                                VerticalGravity="Center"
                                Text="@String/MX_LANG_NAVI_GUIDANCE_SETTING_TEXT_Audio_Setup_Setting"
                                TextFont="@Font/UIFont46_Text_R"
                                TextColor="@Color/ListFavColor"
                                VisibleSpeechs="@String/MX_LANG_NAVI_MAP_SETTING_VisibleSpeechs_05"
                                ClickDataCollection="BTN_Audio_Setup_Setting" />
                            <ImageActor
                                Id="@Id/PreDefine/Icon_line_Audio"
                                LayoutHeight="2"
                                LayoutWidth="267"
                                LayoutTopMargin="110"
                                Background="@Drawable/menu_line_DayNight" />
                        </RelativeLayout>
                        <ImageActor
                            Id="@Id/PreDefine/BTN_Audio_Setup_Blank"
                            LayoutHeight="0"
                            LayoutWeight="2"
                            LayoutWidth="267"
                            Visible="Gone" />
                    </LinearLayout>
                    <!-- Group Btn与分割线 -->
                    <LinearLayout
                        Id="@Id/PreDefine/LayoutGroupDriving"
                        LayoutHeight="113"
                        LayoutWidth="267"
                        LayoutTopMargin="-1"
                        Orientation="Vertical">
                        <RelativeLayout
                            LayoutWeight="130"
                            LayoutHeight="0"
                            LayoutWidth="267">
                            <Button
                                Id="@Id/PreDefine/BTN_Group_Driving_Setting"
                                LayoutWidth="267"
                                LayoutHeight="FillParent"
                                PaddingTop="7"
                                Background="@Drawable/menu_bg_2"
                                VerticalGravity="Center"
                                PaddingLeft="3"
                                Text="@String/MX_LANG_NAVI_GUIDANCE_SETTING_TEXT_Group_Driving_Setting"
                                TextFont="@Font/UIFont46_Text_R"
                                TextColor="@Color/ListFavColor"
                                VisibleSpeechs="@String/MX_LANG_NAVI_MAP_SETTING_VisibleSpeechs_06"
                                ClickDataCollection="BTN_Group_Driving_Setting" />
                            <ImageActor
                                Id="@Id/PreDefine/Icon_line"
                                LayoutHeight="2"
                                LayoutWidth="267"
                                LayoutTopMargin="110"
                                Visible="Gone"
                                Background="@Drawable/menu_line_DayNight" />
                        </RelativeLayout>
                        <ImageActor
                            Id="@Id/PreDefine/BTN_Group_Driving_Blank"
                            LayoutWeight="2"
                            LayoutHeight="0"
                            LayoutWidth="267"
                            Visible="Gone" />
                    </LinearLayout>
                    <!-- Charging Btn与分割线 -->
                    <LinearLayout
                        Id="@Id/PreDefine/LayoutCharging"
                        LayoutHeight="113"
                        LayoutWidth="267"
                        LayoutTopMargin="-1"
                        Visible="Gone">
                        <Button
                            Id="@Id/PreDefine/BTN_Charging_Setting"
                            LayoutWidth="267"
                            LayoutHeight="113"
                            PaddingTop="5"
                            PaddingLeft="1"
                            Background="@Drawable/menu_bg_3"
                            VerticalGravity="Center"
                            Text="@String/MX_LANG_NAVI_CHARGING_SETTING_EV_TEXT_Chargingsetting"
                            TextFont="@Font/UIFont46_Text_R"
                            TextColor="@Color/ListFavColor"
                            ClickDataCollection="BTN_Charging_Setting" />
                    </LinearLayout>
                </LinearLayout>
                <RelativeLayout
                    Id="@+Id/SuperListLayout"
                    LayoutWidth="946"
                    LayoutHeight="711"
                    LayoutTopMargin="139"
                    LayoutLeftMargin="333">
                    <SuperList
                        Id="@+Id/SuperList"
                        LayoutWidth="921"
                        LayoutHeight="FillParent"
                        VisibleSpeechs="@String/NAVI_Scroll_Bar_VisibleSpeechs_With_TurnPages_And_Silde_01" />
                    <SmoothListScrollBar
                        Id="@+Id/SmoothListScrollBar"
                        LayoutHeight="703"
                        LayoutWidth="26"
                        LayoutLeftMargin="919"
                        LayoutTopMargin="38"
                        ScrollBarLayout="@Layout/CActorScrollbarNewThirty" />
                </RelativeLayout>
            </RelativeLayout>
        </MXUIScreenRelativeLayout>
    </RelativeLayout>
</Resource>
