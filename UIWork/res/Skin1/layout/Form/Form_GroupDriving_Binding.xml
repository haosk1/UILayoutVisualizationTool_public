<?xml version="1.0" encoding="UTF-8" ?>
<Resource>
    <RelativeLayout
        Id="@Id/PreDefine/LayoutParent"
        LayoutHeight="FillParent"
        LayoutWidth="FillParent">
        <TransparencyActor
            Id="@+Id/TransparencyScreen"
            LayoutHeight="FillParent"
            LayoutWidth="FillParent" />
        <Function
            Id="@Id/PreDefine/Fragment_GroupDrivingBinding"
            LayoutHeight="FillParent"
            LayoutWidth="FillParent" />
        <!--HMI界面布局部分，此元器件LayoutWidth表示半屏尺寸-->
        <MXUIScreenRelativeLayout
            Id="@Id/PreDefine/MXUIScreenRelativeLayout"
            LayoutAlignParentRight="true"
            LayoutWidth="1424"
            LayoutHeight="FillParent">
            <!--SearchBar-->
            <LinearLayout
                Id="@Id/PreDefine/SearchBar"
                LayoutHeight="98"
                LayoutWidth="476"
                LayoutTopMargin="112"
                LayoutLeftMargin="16"
                Background="@Drawable/search_bg"
                Clickable="true">
                <Button
                    Id="@Id/PreDefine/BTN_Search"
                    LayoutHeight="FillParent"
                    LayoutWidth="FillParent"
                    Text="@String/MX_LANG_NAVI_MAP_CCP_VIEW_PNAV1_TEXT_HInt_Text1"
                    TextFont="@Font/UIFont34_Text_L"
                    TextColor="@Color/DayBlack_NightWhite_Grey"
                    PaddingLeft="48"
                    PaddingTop="4"
                    HorizontalGravity="Left"
                    DuplicateParentState="true"
                    ClickDataCollection="BTN_Search" />
            </LinearLayout>
            <!-- 组队绑定画页布局 -->
            <RelativeLayout
                Id="@Id/PreDefine/LayoutMain"
                LayoutHeight="FillParent"
                LayoutWidth="FillParent"
                LayoutTopMargin="348">
                <!-- 背景图片 -->
                <RelativeLayout
                    Id="@+Id/LayoutBG"
                    LayoutHeight="620"
                    LayoutWidth="584"
                    Background="@Drawable/card_bg_day_and_night"
                    Clickable="true">
                    <RelativeLayout
                        Id="@+Id/FullScreen"
                        LayoutWidth="552"
                        LayoutHeight="588"
                        LayoutLeftMargin="16"
                        LayoutTopMargin="16">
                        <!-- 集结文言 -->
                        <TextActor
                            LayoutHeight="52"
                            LayoutWidth="378"
                            LayoutTopMargin="48"
                            LayoutLeftMargin="144"
                            TextFont="@Font/UIFont40_Text_M"
                            TextColor="@Color/BtnFavoriteColor"
                            Text="@String/MX_LANG_NAVI_GROUPDRIVING_LOADING_TEXT_Group_Driving"
                            HorizontalGravity="Left" />
                        <!-- 返回按钮 -->
                        <ImageButton
                            Id="@+Id/BtnClose"
                            LayoutWidth="116"
                            LayoutHeight="116"
                            LayoutTopMargin="14"
                            LayoutLeftMargin="14"
                            Background="@Drawable/btn_h84_background_normal_prox"
                            Drawable="@Drawable/icon_close_s"
                            VisibleSpeechs="@String/MX_LANG_NAVI_GROUPDRIVING_BINDING_VisibleSpeechs_01"
                            ClickDataCollection="BTN_Close" />
                        <!--Loading部品-->
                        <RelativeLayout
                            Id="@+Id/Navi_Qr_Code_Loading_Layout"
                            LayoutHeight="FillParent"
                            LayoutWidth="FillParent">
                            <RelativeLayout
                                LayoutWidth="200"
                                LayoutHeight="200"
                                LayoutTopMargin="160"
                                HorizontalGravity="Center"
                                LayoutCenterHorizontal="true"
                                Background="@Drawable/QR_Code_samll_bg_mask_card_place">
                                <AutomaticImageActor
                                    Id="@+Id/Navi_Qr_Code_Loading"
                                    LayoutWidth="FillParent"
                                    LayoutHeight="FillParent"
                                    ImageChangeTimer="83"
                                    Cyclable="true"
                                    LayoutCenterHorizontal="true"
                                    Drawable="@Drawable/loading_anim_seq_l_LayerList"
                                    Visible = "Gone" />
                            </RelativeLayout>
                            <TextActor
                                LayoutWidth="FillParent"
                                LayoutTopMargin="385"
                                LayoutHeight="52"
                                SingleLine="true"
                                HorizontalGravity="Center"
                                TextColor="@Color/DayBlack_NightWhite"
                                TextFont="@Font/UIFont34_Text_L"
                                Text="@String/MX_LANG_NAVI_TENCENT_MY_CAR_LOADING_TEXT_QR_Code_Loading" />
                        </RelativeLayout>
                        <!--二维码部品-->
                        <RelativeLayout
                            Id="@+Id/Relative_ShowQRCode"
                            LayoutWidth="FillParent"
                            LayoutHeight="FillParent"
                            Visible="Gone">
                            <RelativeLayout
                                LayoutWidth="188"
                                LayoutHeight="188"
                                LayoutTopMargin="160"
                                LayoutLeftMargin="182"
                                Background="@Drawable/QR_Code_small_bg" >
                                <ImageActor
                                    Id="@+Id/QrBindingCode"
                                    LayoutWidth="178"
                                    LayoutHeight="178"
                                    LayoutCenterInParent = "true"/>
						    </RelativeLayout>
                            <TextActor
                                LayoutHeight="WrapContent"
                                LayoutWidth="492"
                                LayoutTopMargin="390"
                                PaddingLeft="2"
                                LayoutCenterHorizontal="true"
                                TextColor="@Color/BtnFavoriteColor"
                                TextFont="@Font/UIFont34_Text_L"
                                Text="@String/MX_LANG_NAVI_TENCENT_MY_CAR_TEXT_No_Bind"
                                LineIntervalMultiFactor="0.1"
                                LineIntervalAddFactor="-6"
                                SingleLine="false"
                                WordWrapped="true" />
                        </RelativeLayout>
                        <!--获取失败部品-->
                        <RelativeLayout
                            Id="@+Id/Relative_Fail"
                            LayoutWidth="FillParent"
                            LayoutHeight="FillParent"
                            Visible="Gone">
                            <RelativeLayout
                                LayoutWidth="200"
                                LayoutHeight="200"
                                LayoutTopMargin="160"
                                HorizontalGravity="Center"
                                LayoutCenterHorizontal="true"
                                Background="@Drawable/QR_Code_samll_bg_mask_card_place">
                                <ImageButton
                                    Id="@+Id/BTN_Fail_Load"
                                    LayoutWidth="FillParent"
                                    LayoutHeight="FillParent"
                                    Drawable="@Drawable/refresh_erweima" />
                            </RelativeLayout>
                            <TextActor
                                LayoutTopMargin="377"
                                Text="@String/MX_LANG_NAVI_FAVORITE_WECHAT_QR_CODE_FAILED_TEXT_Fail_Load"
                                TextFont="@Font/UIFont34_Text_L"
                                TextColor="@Color/NormalBtn_RGB"
                                HorizontalGravity="Center"
                                LayoutCenterHorizontal="true"
                                LayoutWidth="420"
                                LayoutHeight="114"
                                PaddingLeft="2"
                                SingleLine="false"
                                WordWrapped="true"
                                MultiLineNum="2"
                                LineIntervalMultiFactor="0.0"
                                LineIntervalAddFactor="6" />
                        </RelativeLayout>
                    </RelativeLayout>
                </RelativeLayout>
            </RelativeLayout>
        </MXUIScreenRelativeLayout>
        <!--1558宽度元器件居中-->
        <RelativeLayout
            Id="@Id/PreDefine/CenterLayoutParent"
            LayoutHeight="FillParent"
            LayoutWidth="1314"
            LayoutAlignParentRight="true"
            Clickable="false">
            <RelativeLayout
                Id="@Id/PreDefine/ReturnCCP"
                LayoutWidth="WrapContent"
                LayoutHeight="164"
                LayoutCenterHorizontal="true"
                LayoutAlignParentBottom="true"
                LayoutBottomMargin="94"
                Background="@Drawable/background_96h_hero_prox_day_shadows">
                <RelativeLayout
                    Id="@Id/PreDefine/BtnReturnCCP"
                    LayoutWidth="WrapContent"
                    LayoutHeight="164"
                    LayoutLeftMargin="18"
                    LayoutRightMargin="18"
                    LayoutCenterInParent="true"
                    VisibleSpeechs="@String/MX_LANG_NAVI_MAP_RESTRICTION_VIEW_WITHOUT_GUIDANCE_VisibleSpeechs_02"
                    Clickable="true">
                    <LinearLayout
                        LayoutWidth="WrapContent"
                        LayoutHeight="104"
                        LayoutLeftMargin="12"
                        LayoutRightMargin="12"
                        LayoutCenterInParent="true"
                        DuplicateParentState="true"
                        Background="@Drawable/Gradient_background_96h_hero_prox">
                        <ImageActor
                            LayoutLeftMargin="28"
                            LayoutCenterVertical="true"
                            DuplicateParentState="true"
                            Drawable="@Drawable/back_to_destination_day" />
                        <!--占位-->
                        <ImageActor
                            Id="@Id/PreDefine/SpaceBefore"
                            LayoutWidth="2"
                            LayoutHeight="FillParent"
                            Visible="Gone" />
                        <TextActor
                            Id="@Id/PreDefine/TxtReturnCCP"
                            LayoutHeight="FillParent"
                            LayoutWidth="WrapContent"
                            LayoutLeftMargin="8"
                            LayoutRightMargin="47"
                            PaddingTop="4"
                            DuplicateParentState="true"
                            Text="@String/MX_LANG_NAVI_MAP_CCP_VIEW_PNAV2_TEXT_Back_to_CCP"
                            TextFont="@Font/BtnFont40_N_TextL_P_TextM_C_TextM_D_TextL"
                            TextColor="@Color/HeroBtnText_RGB" />
                        <!--占位-->
                        <ImageActor
                            Id="@Id/PreDefine/SpaceAfter"
                            LayoutWidth="2"
                            LayoutHeight="FillParent"
                            Visible="Gone" />
                    </LinearLayout>
                </RelativeLayout>
            </RelativeLayout>
            <!--车线-->
            <LinearLayout
                Id="@Id/PreDefine/LaneLayout"
                LayoutHeight="164"
                LayoutTopMargin="78"
                LayoutCenterHorizontal="true"
                Background="@Drawable/background_020_m_lane_info_box"
                Visible="Invisible">
                <RelativeLayout
                    LayoutWidth="WrapContent"
                    LayoutHeight="96"
                    LayoutLeftMargin = "34"
                    LayoutRightMargin = "34"
                    LayoutTopMargin="34">
                    <!--推荐车线背景帧动画-->
                    <LaneAutomaticImageActor
                        Id="@Id/PreDefine/LaneAutomatic"
                        LayoutHeight="FillParent"
                        ImageChangeTimer="38"
                        Cyclable="true"
                        Drawable="@Drawable/background_recommend_lane"
                        Visible="Gone" />
                    <LaneInfo
                        Id="@Id/PreDefine/Lane"
                        LayoutHeight="FillParent"
                        LaneRecommended="@Drawable/DrawLevel_RecommendLane"
                        LaneAvailable="@Drawable/DrawLevel_AvailableLane"
                        LaneDisable="@Drawable/DrawLevel_DisableLane"
                        LaneOmit="@Drawable/more"
                        LaneTG="@Drawable/DrawLevel_All_SpecialLane"
                        LaneMiddleImg="@Drawable/background_020_m_lane_info_box_line"
                        LanePaddingLeft="8"
                        LanePaddingRight="8"
                        LanePaddingTop="16"
                        LanePaddingBottom="16"
                        LaneMaxCnt="6"
                        Clickable="true" />
                </RelativeLayout>
            </LinearLayout>
        </RelativeLayout>
    </RelativeLayout>
</Resource>