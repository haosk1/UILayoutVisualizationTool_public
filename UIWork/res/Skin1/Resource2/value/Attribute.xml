		<Attr name = "IntervalBetweenCurPosPercentAndThumb" format = "widthdimension"/>
		<Attr name = "LeftPaddingCurPosPercentToThumb" format = "widthdimension"/>
		<Attr name = "TopPaddingCurPosPercentToThumb" format = "widthdimension"/>
		<Attr name = "RightPaddingCurPosPercentToThumb" format = "widthdimension"/>
		<Attr name = "BottomPaddingCurPosPercentToThumb" format = "widthdimension"/>
		<Attr name = "ShowStyleByPassword">
			<Enum name="true" value="1"/>
			<Enum name="false" value="0"/>	
		</Attr>
		<Attr name = "Cyclable">
			<Enum name="true" value="1"/>
			<Enum name="false" value="0"/>
		</Attr>
		<Attr name = "ImageChangeTimer" format = "integer"/>
		<Attr name = "DirectionArrowsActorColor" format = "reference"/>
		<Attr name = "WipeEnabled">
			<Enum name = "true" value = "1"/>
			<Enum name = "false" value = "0"/>
		</Attr>
		
		<Attr name = "SwappingAnimationEnabled">
			<Enum name = "true" value = "1"/>
			<Enum name = "false" value = "0"/>
		</Attr>
		<Attr name = "ListDeleteAnimation">
			<Enum name = "true" value = "1"/>
			<Enum name = "false" value = "0"/>
		</Attr>
		<Attr name = "TopMenuBtnLayout" format = "reference"/>
