		<Attr name = "ScrollViewBarLayoutLeftMargin" format = "integer"/>
		<Attr name = "ScrollViewBarLayoutTopMargin" format = "integer"/>
		<Attr name = "ScrollViewBarLayoutMinWidth" format = "integer"/>
		<Attr name = "ScrollViewBarLayoutMinHeight" format = "integer"/>
