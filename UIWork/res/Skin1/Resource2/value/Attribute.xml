		<Attr name = "TextureResidual" format = "integer"/>
		<Attr name = "LeftTopSubBackground" format = "reference"/>
		<Attr name = "RightTopSubBackground" format = "reference"/>
		<Attr name = "RightBottomSubBackground" format = "reference"/>
 		<Attr name = "CurPosPercentFont" format = "reference"/>
		<Attr name = "CurPosPercentColor" format = "reference"/>
		<Attr name = "DrawCurPosPercent">
			<Enum name="true" value="1"/>
			<Enum name="false" value="0"/>	
		</Attr>
		<Attr name = "DrawCurPosPercentPosition">
			<Enum name="CenterOfThumb" value="0"/>
			<Enum name="LeftOfThumb" value="1"/>
			<Enum name="TopOfThumb" value="2"/>
			<Enum name="RightOfThumb" value="3"/>
			<Enum name="BottomOfThumb" value="4"/>	
